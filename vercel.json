{
  "buildCommand": "",
  "rewrites": [
    {
      "source": "/fed-dashboard/:path*",
      "destination": "/fed-dashboard/:path*"
    },
    {
      "source": "/api/h41/dates/:path*",
      "destination": "/api/h41/dates/:path*"
    },
    {
      "source": "/api/h41/report",
      "destination": "/api/h41/report"
    },
    {
      "source": "/api/h41/compare",
      "destination": "/api/h41/compare"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=600, stale-while-revalidate=3600"
        }
      ]
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "content-type",
          "value": "(text/css|application/javascript|font/.*)"
        }
      ],
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
