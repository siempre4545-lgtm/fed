{
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/h41/summary",
      "destination": "/api/index"
    },
    {
      "source": "/api/h41/interpretation",
      "destination": "/api/index"
    },
    {
      "source": "/api/h41/detail",
      "destination": "/api/index"
    },
    {
      "source": "/api/h41/weekly-summary",
      "destination": "/api/index"
    },
    {
      "source": "/api/h41/history",
      "destination": "/api/index"
    },
    {
      "source": "/api/h41.txt",
      "destination": "/api/index"
    },
    {
      "source": "/api/h41",
      "destination": "/api/index"
    },
    {
      "source": "/economic-indicators",
      "destination": "/api/index"
    },
    {
      "source": "/secret-indicators",
      "destination": "/api/index"
    },
    {
      "source": "/concepts",
      "destination": "/api/index"
    },
    {
      "source": "/levels",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=600, stale-while-revalidate=3600"
        }
      ]
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "content-type",
          "value": "(text/css|application/javascript|font/.*)"
        }
      ],
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
