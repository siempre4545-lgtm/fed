{
  "name": "h41-dashboard",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node --enable-source-maps dist/index.js",
    "build": "tsc -p tsconfig.json",
    "report": "tsx src/notify.ts",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No ESLint configured. Run typecheck instead.' && npm run typecheck",
    "audit:local": "tsx scripts/audit-site.ts",
    "audit:prod": "BASE_URL=https://fedreportsh.vercel.app tsx scripts/audit-site.ts"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "playwright": "^1.40.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}

