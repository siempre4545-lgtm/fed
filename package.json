{
  "name": "h41-dashboard",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx api/index.ts",
    "build": "echo 'No build step required for Express server'",
    "build:tsc": "tsc -p tsconfig.json",
    "report": "tsx src/notify.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "echo 'No ESLint configured. Run typecheck instead.' && npm run typecheck",
    "audit:local": "tsx scripts/audit-site.ts",
    "audit:prod": "BASE_URL=https://fedreportsh.vercel.app tsx scripts/audit-site.ts"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "express": "^4.19.2",
    "next": "^14.2.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^4.0.17"
  }
}
